{
    "manifest_version": 2,
    "name": "Tab Saver",
    "version": "1.0",
    "description": "Saves all tabs on a Firefox window and reopen saved windows",
    "icons": {
        "48": "icons/tabsaver_48.png",
        "96": "icons/tabsaver_96.png"
    },
    "permissions": [
        "tabs",
        "sessions",
        "storage"
    ],
    "browser_action": {
        "default_icon": "icons/tabsaver_32.png",
        "default_title": "tabsaver",
        "default_popup": "tabsaver.html"
    }
}